@using MvcKutuphane.Models.Entity
@model List<Tbl_mesajlar>
@{
    ViewBag.Title = "Giden";
    Layout = "~/Views/Shared/_LayoutPanel.cshtml";
}

<!DOCTYPE html>
<html>
<head>

</head>
<body>

    <h3>Giden Mesajlar Sayfası</h3>
    <br />
    <table class="table table-bordered">
        <tr>
            <th>Alıcı</th>
            <th>Konu</th>
            <th>Mesaj</th>
            <th>Tarih</th>
        </tr>
        @foreach (var x in Model)
        {
            <tr>
                <td>@x.Alıcı</td>
                <td>@x.Konu</td>
                <td>@x.Icerik</td>
                <td>@x.Tarih</td>
            </tr>
        }
    </table>
</body>
</html>

